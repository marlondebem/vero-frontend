{
  "extends": [
    "next",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier" : ["error", {
      "printWidth": 80,
      "tabWidth": 2,
      "singleQuote": true,
      "jsxSingleQuote": true,
      "trailingComma": "all",
      "arrowParens": "always",
      "semi": false,
      "endOfLine": "auto"
      }]
    }
}
  